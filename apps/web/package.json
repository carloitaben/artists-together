{
  "name": "@artists-together/web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "tsc --noEmit",
    "lint": "next lint"
  },
  "dependencies": {
    "@ark-ui/react": "^5.0.0",
    "@artists-together/core": "workspace:*",
    "@conform-to/dom": "^1.3.0",
    "@conform-to/react": "^1.3.0",
    "@conform-to/valibot": "^0.0.1",
    "@react-three/fiber": "^9.1.0",
    "@standard-cookie/next": "^0.0.3",
    "@tanstack/query-broadcast-client-experimental": "^5.67.2",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-query-devtools": "^5.67.2",
    "@tanstack/react-query-next-experimental": "^5.67.2",
    "arctic": "^3.5.0",
    "cva": "beta",
    "dayjs": "^1.11.10",
    "lottie-web": "^5.12.2",
    "motion": "^12.5.0",
    "negotiator": "^1.0.0",
    "next": "canary",
    "partysocket": "^1.0.3",
    "perfect-cursors": "^1.0.5",
    "radashi": "^12.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-merge-refs": "^2.1.1",
    "server-only": "^0.0.1",
    "split-type": "^0.3.4",
    "three": "^0.174.0",
    "valibot": "^1.0.0"
  },
  "devDependencies": {
    "@artists-together/config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.67.2",
